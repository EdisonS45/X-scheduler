{
  "rewrites": [
    {
      "source": "/api/projects/(.*)/upload-csv",
      "destination": "/api/uploadProxy"
    },
    {
      "source": "/api/templates/(.*)/create-project",
      "destination": "/api/uploadProxy"
    },
    
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy"
    },
    
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}