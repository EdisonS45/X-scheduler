{
  "rewrites": [
    {
      "source": "/api/templates/:id/create-project",
      "destination": "/api/uploadProxy?path=templates/:id/create-project"
    },
    {
      "source": "/api/projects/:id/upload-csv",
      "destination": "/api/uploadProxy?path=projects/:id/upload-csv"
    },
    
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy?path=$1"
    },
    
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}